{
  "name": "pothos-plugin-effect",
  "description": "Seamless integration between the Pothos GraphQL and Effect.",
  "publishConfig": {
    "access": "public",
    "registiry": "https://registry.npmjs.org"
  },
  "version": "0.8.0",
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "type": "module",
  "main": "./dist/core/index.mjs",
  "types": "./dist/core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/core/index.d.ts",
      "require": "./dist/core/index.mjs",
      "import": "./dist/core/index.mjs"
    },
    "./relay": {
      "types": "./dist/relay/index.d.ts",
      "require": "./dist/relay/index.mjs",
      "import": "./dist/relay/index.mjs"
    },
    "./prisma": {
      "types": "./dist/prisma/index.d.ts",
      "require": "./dist/prisma/index.mjs",
      "import": "./dist/prisma/index.mjs"
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "nanobundle build",
    "prepack": "yarn build"
  },
  "keywords": [
    "effect",
    "pothos",
    "plugin"
  ],
  "author": {
    "name": "Chanhee Lee",
    "email": "contact@imch.dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@pothos/core": "3.41.0",
    "@pothos/plugin-errors": "3.11.1",
    "@pothos/plugin-relay": "3.46.0",
    "@types/graphql": "14.5.0",
    "@types/node": "20.11.13",
    "@vitest/coverage-v8": "1.2.2",
    "effect": "2.2.2",
    "nanobundle": "2.0.0",
    "prisma": "^5.9.1",
    "typescript": "5.4.0-beta",
    "vitest": "1.2.2"
  },
  "peerDependencies": {
    "@pothos/core": "^3",
    "effect": ">=2.2.0 <3",
    "graphql": "^16 || ^17"
  },
  "dependencies": {
    "@pothos/plugin-prisma": "^3.63.1",
    "@prisma/client": "5.9.1",
    "tiny-invariant": "^1.3.1",
    "type-plus": "^7.6.0"
  },
  "packageManager": "yarn@4.1.0"
}
