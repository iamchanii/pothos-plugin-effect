{
  "name": "pothos-plugin-effect",
  "description": "Seamless integration between the Pothos GraphQL and Effect.",
  "publishConfig": {
    "access": "public",
    "registiry": "https://registry.npmjs.org"
  },
  "version": "1.2.0",
  "files": [
    "package.json",
    "README.md",
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.mjs",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "test": "yarn prisma generate && vitest",
    "build": "nanobundle build",
    "ci": "yarn test --coverage && yarn build",
    "prepack": "yarn build",
    "publish": "yarn npm publish"
  },
  "keywords": [
    "effect",
    "pothos",
    "plugin"
  ],
  "author": {
    "name": "Chanhee Lee",
    "email": "contact@imch.dev"
  },
  "license": "MIT",
  "devDependencies": {
    "@pothos/core": "3.41.0",
    "@pothos/plugin-errors": "3.11.1",
    "@pothos/plugin-prisma": "3.63.1",
    "@pothos/plugin-relay": "3.46.0",
    "@prisma/client": "5.9.1",
    "@types/better-sqlite3": "^7.6.9",
    "@types/node": "20.11.13",
    "@vitest/coverage-v8": "*",
    "better-sqlite3": "^9.4.0",
    "drizzle-kit": "^0.20.14",
    "drizzle-orm": "^0.29.3",
    "effect": "^3.0.0",
    "graphql": "*",
    "nanobundle": "*",
    "prisma": "5.9.1",
    "typescript": "*",
    "vitest": "*"
  },
  "peerDependencies": {
    "@pothos/core": "^3",
    "effect": ">=3.0.0 <4",
    "graphql": "^16 || ^17"
  },
  "dependencies": {
    "effect-utils": "workspace:^",
    "type-plus": "^7.6.0"
  },
  "packageManager": "yarn@4.1.0"
}
